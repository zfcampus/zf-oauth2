{
    "name": "zfcampus/zf-oauth2",
    "description": "ZF2 module for implementing an OAuth2 server",
    "license": "BSD-3-Clause",
    "keywords": [
        "api",
        "oauth2",
        "framework",
        "zf2"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev",
            "dev-develop": "1.4-dev"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "bshaffer/oauth2-server-php": "^1.8",
        "zendframework/zend-crypt": "^2.6 || ^3.0",
        "zendframework/zend-http": "^2.5.4",
        "zendframework/zend-mvc": "^2.7.9 || ^3.0.2",
        "zendframework/zend-servicemanager": "^2.7.6 || ^3.1",
        "zfcampus/zf-api-problem": "^1.2.1",
        "zfcampus/zf-content-negotiation": "^1.2.1"
    },
    "require-dev": {
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "^4.8 || ^5.4",
        "squizlabs/php_codesniffer": "^2.3.1",
        "zendframework/zend-authentication": "^2.5.3",
        "zendframework/zend-db": "^2.8.1",
        "zendframework/zend-i18n": "^2.7.3",
        "zendframework/zend-modulemanager": "^2.7.2",
        "zendframework/zend-test": "^2.6.1 || ^3.0.1"
    },
    "autoload": {
        "psr-4": {
            "ZF\\OAuth2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZFTest\\OAuth2\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
