{
    "name": "zfcampus/zf-oauth2",
    "description": "ZF2 module for implementing an OAuth2 server",
    "license": "BSD-3-Clause",
    "keywords": [
        "api",
        "oauth2",
        "framework",
        "zf2",
        "doctrine"
    ],
    "repositories": [
        { "type": "composer", "url": "https://packages.zendframework.com/" }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "bshaffer/oauth2-server-php": "~1.3",
        "zendframework/zend-crypt": "~2.3",
        "zendframework/zend-http": "~2.3",
        "zendframework/zend-mvc": "~2.3",
        "zendframework/zend-servicemanager": "~2.3",
        "zfcampus/zf-api-problem": "~1.0",
        "zfcampus/zf-content-negotiation": "~1.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "3.7.*",
        "squizlabs/php_codesniffer": "~2",
        "zendframework/zendframework": ">=2.3.2,<3.0.0",
        "doctrine/doctrine-orm-module": "*",
        "phpro/zf-doctrine-hydration-module": ">=0.1.5",
        "zfcampus/zf-apigility": "~1.0",
        "zfcampus/zf-apigility-documentation": "~1.0",
        "zfcampus/zf-apigility-doctrine": "~0.3",
        "zf-commons/zfc-user-doctrine-orm": "~1.0",
        "hounddog/doctrine-data-fixture-module": "0.0.*"
    },
    "autoload": {
        "psr-4": {
            "ZF\\OAuth2\\": "src/",
            "ZFTest\\OAuth2\\": "test/"
        },
        "classmap": [
            "Module.php"
        ]
    }
}
