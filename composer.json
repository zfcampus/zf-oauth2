{
    "name": "zfcampus/zf-oauth2",
    "description": "ZF2 module for implementing an OAuth2 server",
    "license": "BSD-3-Clause",
    "keywords": [
        "api",
        "oauth2",
        "framework",
        "zf2"
    ],
    "repositories": [
        { "type": "composer", "url": "https://packages.zendframework.com/" },
        {
            "type": "vcs",
            "url": "https://github.com/AGmakonts/zf-api-problem.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/AGmakonts/zf-content-negotiation.git"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev",
            "dev-develop": "1.4-dev"
        }
    },
    "require": {
        "bshaffer/oauth2-server-php": "1.8.*",
        "zendframework/zend-crypt": "2.6.*",
        "zendframework/zend-http": "2.5.*",
        "zendframework/zend-mvc": "3.0.*",
        "zendframework/zend-servicemanager": "3.1.*",
        "zfcampus/zf-api-problem": "dev-feature/zf3-support",
        "zfcampus/zf-content-negotiation": "dev-feature/zf3"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*",
        "squizlabs/php_codesniffer": "^2.3.1",
        "zendframework/zend-test": "3.0.*",
        "zendframework/zend-authentication": "2.5.*",
        "zendframework/zend-i18n": "2.7.*",
        "zendframework/zend-db": "2.8.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "ZF\\OAuth2\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ZFTest\\OAuth2\\": "test/"
        }
    }
}
