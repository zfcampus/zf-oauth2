{
    "name": "zfcampus/zf-oauth2",
    "description": "ZF2 module for implementing an OAuth2 server",
    "license": "BSD-3-Clause",
    "keywords": [
        "api",
        "oauth2",
        "framework",
        "zf2"
    ],
    "repositories": [
        { "type": "composer", "url": "https://packages.zendframework.com/" }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "bshaffer/oauth2-server-php": "dev-develop",
        "zendframework/zendframework": "~2.3",
        "zfcampus/zf-api-problem": "~1.0-dev",
        "zfcampus/zf-content-negotiation": "~1.0-dev"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "*@dev",
        "phpunit/PHPUnit": "3.7.*",
        "satooshi/php-coveralls": ">=0.6.0"
    },
    "autoload": {
        "psr-4": {
            "ZF\\OAuth2\\": "src/",
            "ZFTest\\OAuth2\\": "test/"
        },
        "classmap": [
            "Module.php"
        ]
    }
}
